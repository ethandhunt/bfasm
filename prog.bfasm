#define \n $10
#include stdlib/stdio.bfasm

.start
    alloc @a
    alloc @b
    > @a {,}
    .setv @b @a
    -print "a: "
    -printv @a
    -print \n "b: "
    -printv @b
    -print \n


.setv %a %b
    ;a = b
    alloc @temp0
    > %a {[-]}
    > %b {[} %a {+} @temp0 {+} %b {-]}
    > @temp0 {[} %b {+} @temp0 {-]}
    ; set @temp0 to 0
    > @temp0 {[-]}
    free @temp0
